        <ReportSummaryBand Name="Summary1" Width="718.2" Height="18.9">
          <TextObject Name="TotalLabel" Left="0" Width="200" Height="18.9" Text="Total QuantityProduced:" Font="Arial, 10pt, style=Bold"/>
