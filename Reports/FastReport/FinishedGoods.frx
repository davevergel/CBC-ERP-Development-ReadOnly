    </ColumnHeaderBand>

    <DataBand Name="Data1" Width="718.2" Height="18.9" DataSource="ReportData">
      <TextObject Name="TextBatchNumber" Left="0" Top="0" Width="100" Height="18.9" Text="[ReportData.BatchNumber]" /><TextObject Name="TextProductName" Left="100" Top="0" Width="100" Height="18.9" Text="[ReportData.ProductName]" /><TextObject Name="TextRoastLevel" Left="200" Top="0" Width="100" Height="18.9" Text="[ReportData.RoastLevel]" /><TextObject Name="TextQuantityProduced" Left="300" Top="0" Width="100" Height="18.9" Text="[ReportData.QuantityProduced]" /><TextObject Name="TextDateProduced" Left="400" Top="0" Width="100" Height="18.9" Text="[ReportData.DateProduced]" />
